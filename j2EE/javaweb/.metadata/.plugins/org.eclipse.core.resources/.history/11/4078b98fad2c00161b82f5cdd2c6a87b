package itat.zttc.shop.model;

public @interface ShopDi {
	/**
	 * 表示这个Annotation 加了一个属性值，如果没有定义default，必须在Annotation中定义
	 * 这个属性
	 * @ShopDi(abc="aaa")
	 * @return
	 */
	String abc() default "";
	
	/**
	 * value 是Annotation的默认属性，在定义是可以不用value=""定义
	 * 而直接通过ShopDi("hello")
	 * @return
	 */
	String value() default "";
}
